# Zoom Apps SDK Configuration
# Copy this file to .env and fill in your Zoom app credentials

# Your Zoom App Client ID (from Zoom Marketplace)
VITE_ZOOM_APP_CLIENT_ID=your_client_id_here

# Your Zoom App Client Secret (from Zoom Marketplace)
VITE_ZOOM_APP_CLIENT_SECRET=your_client_secret_here

# Redirect URI (must match Zoom app settings)
VITE_ZOOM_REDIRECT_URI=https://localhost:5173/auth/callback

# Backend API URL (when you connect to backend)
VITE_API_URL=https://api.meetingsync.com

# Environment
VITE_ENV=development
